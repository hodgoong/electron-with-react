{
    "name": "babel-setup",
    "version": "1.0.0",
    "description": "",
    "main": "dist/app/main.js",
    "scripts": {
        "build": "node scripts/build.prod.js && webpack --config webpack.config.js --optimize-minimize --optimize-dedupe",
        "build:dev": "node scripts/build.dev.js && webpack --config webpack.config.dev.js",
        "web": "npm run build && node scripts/server.prod.js",
        "web:dev": "babel-node scripts/server.dev.js",
        "lint": "esw webpack.config.js src",
        "lint:watch": "npm run lint -- --watch",
        "start": "npm run build && electron .",
        "start:dev": "npm run build:dev && electron ."
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-latest": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^3.6.1",
        "css-loader": "^1.0.0",
        "electron": "^2.0.6",
        "eslint": "^5.2.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-watch": "^4.0.2",
        "express": "^4.16.3",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "mkdirp": "^0.5.1",
        "opn": "^5.3.0",
        "style-loader": "^0.21.0",
        "webpack": "^4.16.4",
        "webpack-cli": "^3.1.0",
        "webpack-dev-middleware": "^3.1.3"
    },
    "dependencies": {
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "redux": "^4.0.0"
    }
}
